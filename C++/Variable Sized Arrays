#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n, q;
    cin>>n>>q;
    int m,v;
    vector<int> a[n];
    for(int i=0; i<n;i++){
        cin>>m;
        for(int j=0;j<m; j++){
            cin>>v;
            a[i].push_back(v);
        }
    }
    int r,s;
    for(int k=0;k<q;k++){
        cin>>r>>s;
        cout<<a[r][s]<<endl;
    }
    return 0;
}
